<%- contentFor('body') %>
<div class="jumbotron text-center">
    <h2>Lista de ASKs<h2>
    <table border="1">
        <thead>
            <tr>
            <td>UUID</td>
            <td>Nivel</td>
            <td>Revis√£o</td>
            <td>Pergunta</td>
            <td>Resposta</td>
            </tr>
        </thead>
        <tbody>
            <% asks.forEach(function(ask) { %>
            <tr>
                <td><%= ask.uuid %></td>
                <td><%= ask.difficulty %></td>
                <td><%= ask.revised %></td>
                <td><%= ask.question.text %></td>
                <td><%= ask.question.answer %></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>